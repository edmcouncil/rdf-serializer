	<owl:Class
		rdf:about="&fibo-be-ptr-ptr;EquityApportionmentTermsSet">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-agr-ctr;ContractTermsSet"/>
		<rdfs:label>equity apportionment terms set</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">Terms setting out the apportionment of equity in some formal business organization.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-be-ptr-ptr;GeneralPartner">
		<rdfs:subClassOf
			rdf:resource="&fibo-be-oac-cpty;DeJureControllingInterestParty"/>
		<rdfs:subClassOf
			rdf:resource="&fibo-be-ptr-ptr;Partner"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onClass
					rdf:resource="&fibo-be-le-lp;LegalPerson"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;hasIdentity"/>
				<owl:qualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-pty-rl;isPlayedBy"/>
				<owl:someValuesFrom>
					<owl:Restriction>
						<owl:allValuesFrom
							rdf:resource="&fibo-fnd-law-lcap;LiabilityCapacity"/>
						<owl:onProperty
							rdf:resource="&fibo-fnd-law-lcap;hasCapacity"/>
					</owl:Restriction>
				</owl:someValuesFrom>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-pty-rl;isPlayedBy"/>
				<owl:someValuesFrom>
					<owl:Restriction>
						<owl:allValuesFrom
							rdf:resource="&fibo-be-ptr-ptr;GeneralPartnerEquity"/>
						<owl:onProperty
							rdf:resource="&fibo-be-ptr-ptr;holdsPartnershipEquity"/>
					</owl:Restriction>
				</owl:someValuesFrom>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>general partner</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;string">http://legal-dictionary.thefreedictionary.com/general+partner</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&xsd;string">NYPL Article 8-A, Section 121-101 (f) and (g) defines general and limited partners as &quot;persons&quot; admitted to a limited partnership in accordance with the partnership agreement. Section 121-101 (n) further defines that corporations are considered &quot;persons&quot;. Corporations can therefore be both limited and general partners. The Uniform Limited Partnership Act (Section 102-15) as well as laws of other states confirm that corporations can serve as general and limited partners (e.g. 6 Del. Laws Section 15-101 (16)). IIn the following cases, corporations served as general partners in a partnership: Whitley v. Klauber, 51 N.Y.2d 555; Larson v. Commissioner, 1976 U.S. Tax Ct. LEXIS 118 (T.C. 1976); Dirienzo v. Lichtenstein, 2013 Del. Ch. LEXIS 242 (Del. Ch. Sept. 30, 2013).</fibo-fnd-utl-av:explanatoryNote>
		<owl:disjointWith
			rdf:resource="&fibo-be-ptr-ptr;LimitedPartner"/>
		<skos:definition
			rdf:datatype="&xsd;string">a partner in a partnership, who holds some part of the general partner equity and typically is jointly and severally liable with the other partners for the liabilities incurred by that partnership</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-be-ptr-ptr;GeneralPartnerEquity">
		<rdfs:subClassOf
			rdf:resource="&fibo-be-oac-opty;ControllingEquity"/>
		<rdfs:subClassOf
			rdf:resource="&fibo-be-ptr-ptr;PartnershipEquity"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom
					rdf:resource="&fibo-be-oac-cpty;ConstitutionalDeJureControl"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;confers"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>general partner equity</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">Equity in a partnership held by general partners; this is typically accompanied by full liability capability on the part of the holders of the equity.</skos:definition>
		<skos:editorialNote
			rdf:datatype="&xsd;string">For a typical, non incorporated partnership, this is the only equity in the entity. Other typically incorporated partnerships may have additional limited equity in addition to or instead of this.</skos:editorialNote>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-be-ptr-ptr;GeneralPartnership">
		<rdfs:subClassOf
			rdf:resource="&fibo-be-ptr-ptr;PartnershipWithGeneralPartners"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:minQualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">2</owl:minQualifiedCardinality>
				<owl:onClass
					rdf:resource="&fibo-be-ptr-ptr;GeneralPartner"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;hasMember"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>general partnership</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;anyURI">http://www.investopedia.com/terms/g/generalpartnership.asp</fibo-fnd-utl-av:adaptedFrom>
		<skos:definition
			rdf:datatype="&xsd;string">a partnership in which the partners are jointly and severally liable for liabilities incurred by the entity</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-be-ptr-ptr;JuridicalPersonLimitedPartner">
		<rdfs:subClassOf
			rdf:resource="&fibo-be-ptr-ptr;LimitedPartner"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onClass
					rdf:resource="&fibo-be-le-lp;JuridicalPerson"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;hasIdentity"/>
				<owl:qualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>juridical person limited partner</rdfs:label>
		<owl:disjointWith
			rdf:resource="&fibo-be-ptr-ptr;LegallyCapablePersonLimitedPartner"/>
		<skos:definition
			rdf:datatype="&xsd;string">a limited partner in a partnership, who is and may only be a type of legal entity e.g. corporation, partnership, trust, private limited company etc. (i.e., not a natural person)</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-be-ptr-ptr;LegallyCapablePersonLimitedPartner">
		<rdfs:subClassOf
			rdf:resource="&fibo-be-ptr-ptr;LimitedPartner"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onClass
					rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/AgentsAndPeople/People/LegallyCapablePerson"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;hasIdentity"/>
				<owl:qualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>legally capable person limited partner</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">a limited partner in a partnership, who is and may only be a LegallyCapable Person i.e. a human being</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-be-ptr-ptr;LiabilityApportionmentTermsSet">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-agr-ctr;ContractTermsSet"/>
		<rdfs:label>liability apportionment terms set</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">Terms setting out the apportionment of liabilities for debts or other liabilities accrued by some formal business organization.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-be-ptr-ptr;LimitedLiabilityGeneralPartner">
		<rdfs:subClassOf
			rdf:resource="&fibo-be-ptr-ptr;GeneralPartner"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-pty-rl;isPlayedBy"/>
				<owl:someValuesFrom>
					<owl:Restriction>
						<owl:allValuesFrom
							rdf:resource="&fibo-be-le-lp;LimitedLiability"/>
						<owl:onProperty
							rdf:resource="&fibo-fnd-law-lcap;hasCapacity"/>
					</owl:Restriction>
				</owl:someValuesFrom>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>limited liability general partner</rdfs:label>
		<owl:disjointWith
			rdf:resource="&fibo-be-ptr-ptr;UnlimitedLiabilityGeneralPartner"/>
		<skos:definition
			rdf:datatype="&xsd;string">a class of general partner who has limited liability per the terms of the limited liability limited partnership agreement</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-be-ptr-ptr;LimitedLiabilityPartnership">
		<rdfs:subClassOf
			rdf:resource="&fibo-be-ptr-ptr;PartnershipWithLimitedPartners"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom
					rdf:resource="&fibo-be-ptr-ptr;LimitedPartner"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;hasMember"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>limited liability partnership</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;anyURI">https://en.wikipedia.org/wiki/Limited_liability_partnership</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&xsd;string">In an LLP, one partner is not responsible or liable for another partner&apos;s misconduct or negligence.</fibo-fnd-utl-av:explanatoryNote>
		<skos:definition
			rdf:datatype="&xsd;string">a partnership in which some or all partners (depending on the jurisdiction) have limited liabilities</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-be-ptr-ptr;LimitedPartner">
		<rdfs:subClassOf
			rdf:resource="&fibo-be-ptr-ptr;Partner"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-pty-rl;isPlayedBy"/>
				<owl:someValuesFrom>
					<owl:Restriction>
						<owl:allValuesFrom
							rdf:resource="&fibo-be-ptr-ptr;LimitedPartnerEquity"/>
						<owl:onProperty
							rdf:resource="&fibo-be-ptr-ptr;holdsPartnershipEquity"/>
					</owl:Restriction>
				</owl:someValuesFrom>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-pty-rl;isPlayedBy"/>
				<owl:someValuesFrom>
					<owl:Restriction>
						<owl:allValuesFrom
							rdf:resource="&fibo-be-le-lp;LimitedLiability"/>
						<owl:onProperty
							rdf:resource="&fibo-fnd-law-lcap;hasCapacity"/>
					</owl:Restriction>
				</owl:someValuesFrom>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>limited partner</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">A partner in a partnership whose liabilities are limited to the extent of their equity holding or guarantees</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-be-ptr-ptr;LimitedPartnerEquity">
		<rdfs:subClassOf
			rdf:resource="&fibo-be-ptr-ptr;PartnershipEquity"/>
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-acc-aeq;IssuedEquity"/>
		<rdfs:label>limited partner equity</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">equity in a partnership held by limited partners; this is not accompanied by any liability capability on the part of the holders of this equity.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-be-ptr-ptr;LimitedPartnership">
		<rdfs:subClassOf
			rdf:resource="&fibo-be-ptr-ptr;PartnershipWithGeneralPartners"/>
		<rdfs:subClassOf
			rdf:resource="&fibo-be-ptr-ptr;PartnershipWithLimitedPartners"/>
		<rdfs:label>limited partnership</rdfs:label>
		<fibo-fnd-utl-av:definitionOrigin
			rdf:datatype="&xsd;anyURI">http://en.wikipedia.org/wiki/Limited_partnership</fibo-fnd-utl-av:definitionOrigin>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&xsd;string">It is a partnership in which only one partner is required to be a general partner.</fibo-fnd-utl-av:explanatoryNote>
		<skos:definition
			rdf:datatype="&xsd;string">a form of partnership similar to a general partnership, except that in addition to one or more general partners (GPs), there are one or more limited partners (LPs)</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-be-ptr-ptr;Partner">
		<rdfs:subClassOf
			rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganizationMember"/>
		<rdfs:subClassOf
			rdf:resource="&fibo-be-oac-opty;ConstitutionalOwner"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onClass
					rdf:resource="&fibo-be-le-lp;LegalPerson"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;hasIdentity"/>
				<owl:qualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-pty-rl;isPlayedBy"/>
				<owl:someValuesFrom>
					<owl:Restriction>
						<owl:onClass
							rdf:resource="&fibo-be-ptr-ptr;PartnershipAgreement"/>
						<owl:onProperty
							rdf:resource="&fibo-fnd-pty-pty;isAPartyTo"/>
						<owl:qualifiedCardinality
							rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
					</owl:Restriction>
				</owl:someValuesFrom>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-pty-rl;isPlayedBy"/>
				<owl:someValuesFrom>
					<owl:Restriction>
						<owl:allValuesFrom
							rdf:resource="&fibo-be-ptr-ptr;Partnership"/>
						<owl:onProperty
							rdf:resource="&fibo-fnd-rel-rel;isMemberOf"/>
					</owl:Restriction>
				</owl:someValuesFrom>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>partner</rdfs:label>
		<fibo-fnd-utl-av:definitionOrigin
			rdf:datatype="&xsd;anyURI">http://www.investorwords.com/3608/partner.html</fibo-fnd-utl-av:definitionOrigin>
		<skos:definition
			rdf:datatype="&xsd;string">a member of a partnership</skos:definition>
		<skos:scopeNote
			rdf:datatype="&xsd;string">This term is not referred to directly in specific kinds of partnership. Nearly all partnerships have General Partners, while forms of (mostly legally incorporated) partnerships have in addition Limited Partners. This term is the common ancestor of both.</skos:scopeNote>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-be-ptr-ptr;Partnership">
		<rdfs:subClassOf
			rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
		<rdfs:subClassOf
			rdf:resource="&fibo-be-le-lp;JuridicalPerson"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom
					rdf:resource="&fibo-be-ptr-ptr;Partnership"/>
				<owl:onProperty
					rdf:resource="&fibo-be-le-lp;allowsUnlimitedLiabilityOnEntity"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:minQualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
				<owl:onClass
					rdf:resource="&fibo-be-le-lp;ProfitObjective"/>
				<owl:onProperty
					rdf:resource="&fibo-be-le-lp;hasObjective"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:minQualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
				<owl:onClass
					rdf:resource="&fibo-be-ptr-ptr;PartnershipAgreement"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;isGovernedBy"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:minQualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
				<owl:onClass
					rdf:resource="&fibo-be-ptr-ptr;PartnershipEquity"/>
				<owl:onProperty
					rdf:resource="&fibo-be-le-fbo;hasEquity"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:minQualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">2</owl:minQualifiedCardinality>
				<owl:onClass
					rdf:resource="&fibo-be-ptr-ptr;Partner"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;hasMember"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-pty-pty;hasPartyInRole"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-be-ptr-ptr;Partner"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>partnership</rdfs:label>
		<fibo-fnd-utl-av:definitionOrigin
			rdf:datatype="&xsd;anyURI">http://www.investopedia.com/terms/p/partnership.asp</fibo-fnd-utl-av:definitionOrigin>
		<skos:definition
			rdf:datatype="&xsd;string">a business organization in which two or more individuals manage and operate the business</skos:definition>
		<skos:editorialNote
			rdf:datatype="&xsd;string">If the partnership doesn&apos;t limit the liability of the partners then the party to the contract is a natural person. If it does, then it&apos;s an artificial legal person (Body Corporate) - see Legally Incorporated Partnerships. Scope Note: This term of Partnership in the most general sense is ancestral to both of those.</skos:editorialNote>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-be-ptr-ptr;PartnershipAgreement">
		<rdfs:subClassOf
			rdf:resource="&fibo-be-le-fbo;OrganizationCoveringAgreement"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-pty-pty;hasPartyInRole"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-be-ptr-ptr;Partner"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>partnership agreement</rdfs:label>
		<fibo-fnd-utl-av:definitionOrigin
			rdf:datatype="&xsd;anyURI">http://www.investorguide.com/definition/partnership-agreement.html</fibo-fnd-utl-av:definitionOrigin>
		<fibo-fnd-utl-av:synonym
			rdf:datatype="&xsd;string">articles of partnership</fibo-fnd-utl-av:synonym>
		<skos:definition
			rdf:datatype="&xsd;string">a document setting up a partnership, giving the details of the business and the amount each partner is contributing to it.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-be-ptr-ptr;PartnershipEquity">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-acc-aeq;StockholdersEquity"/>
